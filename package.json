{
  "name": "cards-against",
  "version": "0.0.0",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "eslint-config-custom": "^0.0.0",
    "prettier": "^3.0.3",
    "turbo": "^1.10.14"
  },
  "engines": {
    "yarn": ">=2.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "@solidjs/router": "^0.8.3"
  }
}
